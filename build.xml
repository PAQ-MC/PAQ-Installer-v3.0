            <zipfileset excludes="META-INF/*.SF" src="Resorces/zip4j_1.3.2.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="Resorces/argo-3.7.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="Resorces/guava-14.0.1.jar"/>
        </jar>
    </target>
</project>
